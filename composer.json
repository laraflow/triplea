{
    "name": "laraflow/triplea",
    "description": "Triple A (Authentication, Authorization, and Accounting)",
    "keywords": [
        "larflow",
        "laravel",
        "triplea"
    ],
    "homepage": "https://github.com/laraflow/triplea",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Hafijul Islam",
            "email": "author@domain.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.1",
        "ext-pdo": "*",
        "illuminate/contracts": "^8.83",
        "laraflow/core": "^0.0.1",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/collision": "^5.11",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.24",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Laraflow\\TripleA\\": "src",
            "Laraflow\\TripleA\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laraflow\\TripleA\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraflow\\TripleA\\TripleAServiceProvider"
            ],
            "aliases": {
                "TripleA": "Laraflow\\TripleA\\Facades\\TripleA"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
